<assembly
	xmlns="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2 http://maven.apache.org/xsd/assembly-1.1.2.xsd">
	<id></id>
	<formats>
		<format>dir</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>
	<baseDirectory>../</baseDirectory>
	
	<fileSets>
		<fileSet>
			<directory>str-query-processor/target</directory>
			<outputDirectory>/</outputDirectory>
			<includes>
				<include>str-query-processor-0.1.jar</include>
				<include>lib/*</include>
				<include>resources/*</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>str-gwt-shower/target</directory>
			<outputDirectory>/</outputDirectory>
			<includes>
				<include>*.war</include>
			</includes>
		</fileSet>
		<!--fileSet> <source>README.txt</source> <outputDirectory>/</outputDirectory> 
			<filtered>true</filtered> </fileSet -->
		<!--file> <source>LICENSE.txt</source> <outputDirectory>/</outputDirectory> 
			</file> <file> <source>NOTICE.txt</source> <outputDirectory>/</outputDirectory> 
			<filtered>true</filtered> </file -->
	</fileSets>
</assembly>